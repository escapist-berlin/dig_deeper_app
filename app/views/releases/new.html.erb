<div class="container">
 <%= simple_form_for [@list, @release] do |f| %>
   <%= f.input :artist %>
   <%= f.input :title %>
   <%= f.input :label %>
   <%= f.input :catalog_number %>
   <%= f.input :format %>
   <%= f.input :released %>
   <%= f.input :styles %>
   <%= f.input :tracklist %>
   <%= f.input :link %>
   <%= f.input :cover_url %>
   <%= f.input :comment %>
   <%= f.input :photo, as: :file %>
   <%= f.submit %>
   <% end %>
 </div>
